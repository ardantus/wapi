# Docker environment configuration
# Copy this to .env and customize for your Docker deployment

# UI Login Credentials
UI_CREDENTIALS=admin:password

# Session Secret (use a strong random string in production)
SESSION_SECRET=your-secret-key-change-this-in-production

# Redis URL (required for docker-compose, optional for local development)
REDIS_URL=redis://redis:6379

# Rate Limit per API key per minute
RATE_LIMIT_PER_MINUTE=120

# Node environment
NODE_ENV=production
